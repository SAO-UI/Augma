  "devDependencies": {
    "@antfu/eslint-config": "^0.10.0",
    "@types/minimist": "^1.2.2",
    "@types/semver": "^7.3.9",
    "@yunyoujun/logger": "^0.1.4",
    "enquirer": "^2.3.6",
    "eslint": "^8.3.0",
    "execa": "^6.0.0",
    "fast-glob": "^3.2.7",
    "gray-matter": "^4.0.3",
    "minimist": "^1.2.5",
    "pnpm": "^6.23.1",
    "semver": "^7.3.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "unplugin-icons": "^0.12.18",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^2.6.14",
