{
  "name": "@augma/monorepo",
  "private": true,
  "version": "0.1.1",
  "packageManager": "pnpm@8.6.7",
  "repository": "https://github.com/YunYouJun/augma.git",
  "scripts": {
    "build:client": "pnpm -C packages/client build",
    "build:lib": "pnpm -C packages/augma build",
    "build": "pnpm -r --filter=./packages/** --parallel run build",
    "dev": "pnpm -C packages/client dev",
    "docs": "pnpm run docs:dev",
    "docs:dev": "vitepress dev packages --host --port 3002",
    "docs:build": "vitepress build packages",
    "docs:serve": "vitepress serve packages",
    "docs:theme": "cross-env NODE_ENV=dev:theme vitepress dev packages",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "release": "ts-node-script scripts/release.ts",
    "typecheck": "vue-tsc --noEmit",
    "test:unit": "vitest",
    "test": "npm run test:unit",
    "update": "ts-node-script scripts/update.ts"
  },
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@augma/types": "workspace:*",
    "@iconify-json/ri": "^1.1.10",
    "@types/minimist": "^1.2.2",
    "@types/semver": "^7.5.0",
    "@types/webxr": "^0.5.2",
    "@unocss/eslint-config": "^0.53.5",
    "cross-env": "^7.0.3",
    "enquirer": "^2.3.6",
    "eslint": "^8.45.0",
    "execa": "7.1.1",
    "fast-glob": "^3.3.0",
    "gray-matter": "^4.0.3",
    "markdown-it-container": "^3.0.0",
    "minimist": "^1.2.8",
    "semver": "^7.5.4",
    "ts-node": "^10.9.1",
    "tsup": "^7.1.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "unocss": "^0.53.5",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.4.4",
    "vite-plugin-pwa": "^0.16.4",
    "vitepress": "1.0.0-beta.5",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "@babylonjs/core": "^6.12.3",
    "@vueuse/core": "^10.2.1",
    "prism-theme-vars": "^0.2.4"
  }
}
