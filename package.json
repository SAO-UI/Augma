    "docs:serve": "vitepress serve packages",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "release": "ts-node-script scripts/release.ts",
    "update": "ts-node-script scripts/update.ts"
  },
  "author": {
    "name": "YunYouJun",
