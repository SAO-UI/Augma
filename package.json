{
  "name": "@augma/monorepo",
  "private": true,
  "version": "0.1.1",
  "repository": "https://github.com/YunYouJun/augma.git",
  "scripts": {
    "build:client": "pnpm build -C packages/client",
    "build:lib": "pnpm build -C packages/augma",
    "build": "tsx scripts/build.ts",
    "dev": "pnpm dev -C packages/client",
    "docs": "pnpm run docs:dev",
    "docs:dev": "vitepress dev packages --host --port 3002",
    "docs:build": "vitepress build packages",
    "docs:serve": "vitepress serve packages",
    "docs:theme": "cross-env NODE_ENV=dev:theme vitepress dev packages",
    "lint": "eslint \"**/*.{vue,ts,js}\"",
    "lint:fix": "eslint \"**/*.{vue,ts,js}\" --fix",
    "release": "ts-node-script scripts/release.ts",
    "typecheck": "vue-tsc --noEmit",
    "update": "ts-node-script scripts/update.ts"
  },
  "author": {
    "name": "YunYouJun",
    "email": "me@yunyoujun.cn",
    "url": "https://www.yunyoujun.cn"
  },
  "license": "MIT",
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.1",
    "@iconify-json/ri": "^1.0.2",
    "@types/minimist": "^1.2.2",
    "@types/semver": "^7.3.9",
    "@yunyoujun/logger": "^0.1.4",
    "cross-env": "^7.0.3",
    "enquirer": "^2.3.6",
    "eslint": "^8.7.0",
    "execa": "^6.0.0",
    "fast-glob": "^3.2.11",
    "gray-matter": "^4.0.3",
    "markdown-it-container": "^3.0.0",
    "minimist": "^1.2.5",
    "semver": "^7.3.5",
    "ts-node": "^10.4.0",
    "tsx": "^3.12.2",
    "typescript": "^4.9.4",
    "unocss": "^0.48.4",
    "unplugin-icons": "^0.13.0",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^4.0.4",
    "vite-plugin-pwa": "^0.14.1",
    "vite-plugin-windicss": "^1.6.2",
    "vitepress": "1.0.0-alpha.38"
  },
  "dependencies": {
    "@vueuse/core": "^9.11.0",
    "prism-theme-vars": "^0.2.2"
  }
}
